doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
        link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css' integrity='sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO' crossorigin='anonymous')
    body 
        table.table
            thead 
                tr  
                    th.text-primary Nome 
                    th(scope='col') Telefone-Whatsapp 
                    th(scope='col') E-mail
                    th(scope='col') CPF
                    th(scope='col') Estado 
                    th(scope='col') Cidade
                    th(scope='col') Data de Nascimento 
                    th(scope='col') Titulo da foto 
                    th(scope='col') Nome do(a) fotogr√°fo(a)
                    th(scope='col') Nome da foto

            tbody 
                each estudante in todosOsEstudantes
                    tr 
                        td #{estudante.nome}
                        td #{estudante.tel_whats}  
                        td #{estudante.email}
                        td #{estudante.cpf}
                        td #{estudante.estado}
                        td #{estudante.cidade}
                        td #{estudante.data_nascimento}
                        td #{estudante.titulo_foto}
                        td #{estudante.nome_fotografa}
                        td 
                            img(src=`/uploads/${estudante.nome_foto}`)
                        
                        